import java.util.Date;

public class DateTime {
	private int day;
	private int month;
	private int year;
	private int hour24;
	private int minutes;
	private String dayOfWeek;
	private Date theDate;

	public DateTime(Date theDay) {
		theDate = theDay;
	}
	public Date getTheDate()
	{
		return theDate;
	}
}
